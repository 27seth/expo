{
  "expo": {
    "name": "sandbox",
    "slug": "sandbox",
    "privacy": "public",
    "sdkVersion": "UNVERSIONED",
    "scheme": "sandbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true,
            "ccacheEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": ""
        }
      ]
    ],
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "experiments": {
      "reactServerComponents": false
    }
  }
}
